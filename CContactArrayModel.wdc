      PROCEDURE Load()
      
      // Local variables
      
      // Delete the former content
      ArrayDeleteAll(m_arrContacts)
      
      // Browse the list of contacts
      FOR EACH Contact 
      	clNewContact is CContactModel
      	
      	// Retrieve the items
      	clNewContact.Load()
      	
      	// Add to table
      	_AddArray(clNewContact)
      	
      END
      
      // The graphical interface will have to be updated
      RequestUpdateUI()
     type : 458752
